{
  "name": "cookie-muncher",
  "version": "0.4.2",
  "description": "Effortless cookie management",
  "author": "Bluzzi",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --minify --format cjs,esm --dts",
    "test": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "tsc --noEmit"
  },
  "homepage": "https://github.com/Bluzzi/cookie-muncher#readme",
  "bugs": {
    "url": "https://github.com/Bluzzi/cookie-muncher/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluzzi/cookie-muncher.git"
  },
  "devDependencies": {
    "@bluzzi/eslint-config": "^1.1.0",
    "@types/node": "^20.1.4",
    "@vitest/ui": "^0.31.0",
    "eslint": "^8.40.0",
    "jsdom": "^22.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.0"
  },
  "eslintConfig": {
    "extends": "@bluzzi"
  },
  "tsup": {
    "clean": true
  },
  "keywords": [
    "cookie",
    "cookies",
    "browser",
    "client",
    "http",
    "https",
    "parser",
    "serializer",
    "amd",
    "cjs",
    "commonjs",
    "esm",
    "typescript",
    "typesafe",
    "rfc6265"
  ]
}
